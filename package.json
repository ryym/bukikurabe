{
  "name": "bukikurabe",
  "version": "1.0.0",
  "description": "Compare weapon specs",
  "scripts": {
    "node": "node",
    "_install": "yarn install --cache-folder .yarn-cache",
    "prestart": "npm run _install",
    "start": "run-s clean build && run-p watch serve",
    "clean": "rm -rf build",
    "build": "babel backend --out-dir build",
    "watch": "node scripts/babel-watch.js",
    "serve": "nodemon --watch build build/server.dev.js",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "db:setup": "run-s db:migrate db:schema:update db:seed:insert",
    "db:migrate": "node scripts/db/migrate.js",
    "db:schema:update": "node scripts/db/update-schema.js",
    "db:seed:insert": "node scripts/db/insert-seed.js",
    "pretest": "NODE_ENV=test yarn db:migrate",
    "test": "NODE_ENV=test ava",
    "test:watch": "yarn test -- --watch",
    "test:back": "yarn test -- 'backend/**/*.test.js'",
    "test:watch:back": "yarn test:back -- --watch"
  },
  "keywords": [
    "Splatoon"
  ],
  "author": "ryym",
  "license": "MIT",
  "engines": {
    "node": "<=6.0.0"
  },
  "dependencies": {
    "co": "^4.6.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "pg": "^6.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sql": "^0.72.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.11",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-ryym": "^1.1.0",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.2.0",
    "node-sass": "^3.10.1",
    "node-watch": "^0.4.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "postcss-loader": "^0.13.0",
    "pre-commit": "^1.1.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "ava": {
    "source": [
      "backend"
    ],
    "require": [
      "babel-register"
    ]
  }
}
